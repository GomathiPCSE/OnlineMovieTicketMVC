@model IEnumerable< MovieTicketBooking.Models.SignUpModel>
@{
    ViewBag.Title = "User Details";
}

<h2 class="heading">User Details</h2>
<table cellspacing="0" border="1">
    <tr>
        <th>First Name </th>
        <th>Last Name </th>
        <th> Mobile Number</th>
        <th>MailId</th>
        <th>Password</th>
    </tr>
    @foreach (MovieTicketBooking.Models.SignUpModel user in Model)
    {
        <tr>
            <td>@user.FirstName</td>
            <td>@user.LastName</td>
            <td>@user.MobileNumber</td>
            <td>@user.MailId</td>
            <td>@user.Password</td>
            <td>
                @Html.ActionLink("Delete", "DeleteUser", new { id = user.UserId },

new { onclick = "return confirm('Are sure wants to delete?');" })
</td>
</tr>
}
</table>

